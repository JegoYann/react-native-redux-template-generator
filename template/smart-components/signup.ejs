'use strict';

import React, { Component } from 'react';
import {
    View
} from 'react-native';

import { connect } from 'react-redux';
import { i18n } from '../locales/i18n';
import { toJS } from '../utils/immutable_to_js';
import { signupAsync } from '../actions/authentication';
<%- "import Dumb from '../dumb-components/"+key+"';" %>

const mapStateToProps = (state) => {
  return {
    labels: {
      <% labels.forEach(label => { %>
      <%- label.key+": i18n('"+label.value+"')," %>
      <% }); %>
    },
    flags: {
    }
  }
}

const mapDispatchToProps = (dispatch) => {
  return {
    onSignupPress: (id, password) => {
      dispatch(signupAsync(id, password))
    },
  }
}

export default connect(
  mapStateToProps,
  mapDispatchToProps
)(toJS(Dumb))
